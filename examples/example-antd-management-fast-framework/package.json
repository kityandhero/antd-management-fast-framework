    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.2",
    "docdash": "^2.0.2",
    "easy-soft-develop": "^2.1.181",
