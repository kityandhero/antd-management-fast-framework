{
  "name": "lerna-antd-management-fast-framework",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "change-nrm-local": "nrm use local && npm run sleep:change-nrm",
    "change-nrm-npm": "nrm use npm && npm run sleep:change-nrm",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "preclean:all": "rimraf ./yarn-error.log && rimraf ./yarn.lock && rimraf ./package-lock.json",
    "clean:all": "lerna clean -y",
    "postclean:all": "npm run clean:env && npm run clean:rollup & npm run clean:core-common && npm run clean:core-component && npm run clean:core-framework && npm run clean:test && npm run clean:demo-example && rimraf ./node_modules",
    "clean:core-common": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:core-component": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:core-framework": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:demo-example": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:env": "cd packages/antd-management-fast-env && npm run clean-file",
    "clean:rollup": "cd packages/antd-management-fast-rollup && npm run clean-file",
    "clean:test": "cd packages/antd-management-fast-test && npm run clean-file",
    "cli": "cd packages/antd-management-fast-cli && node ./bin/cli",
    "commitlint": "npx commitlint --edit",
    "config:env": "node ./develop/assists/env.init.js",
    "precz": "npm run update:cz:flag && git stage -A",
    "cz": "cz",
    "postcz": "git push",
    "postinstall": "npm run config:env",
    "lerna": "lerna",
    "prelerna:b": "npm run change-nrm-local",
    "lerna:b": "npm run clean:all && husky install && git pull && pnpm install",
    "lerna:build": "lerna run --stream --sort build",
    "lerna:create": "lerna create",
    "lerna:publish": "lerna publish --yes",
    "lint:f": "cd packages/antd-management-fast-framework && npm run lint",
    "lint:staged": "npx lint-staged",
    "prencu:all": "npm run change-nrm-local",
    "ncu:all": "npx ncu && npm run ncu:core-cli && npm run ncu:core-common && npm run ncu:core-component && npm run ncu:core-framework && npm run ncu:core-rollup && npm run ncu:test && npm run ncu:demo-example",
    "ncu:core-cli": "cd packages/antd-management-fast-cli && ncu",
    "ncu:core-common": "cd packages/antd-management-fast-common && ncu",
    "ncu:core-component": "cd packages/antd-management-fast-component && ncu",
    "ncu:core-framework": "cd packages/antd-management-fast-framework && ncu",
    "ncu:core-rollup": "cd packages/antd-management-fast-rollup && ncu",
    "ncu:demo-example": "cd packages/example-antd-management-fast-framework && ncu",
    "ncu:test": "cd packages/antd-management-fast-test && npx ncu",
    "prencu:u:all": "npm run change-nrm-local",
    "ncu:u:all": "npx ncu -u && npm run ncu:u:core-cli && npm run ncu:u:core-common && npm run ncu:u:core-component && npm run ncu:u:core-framework && npm run ncu:u:core-rollup && npm run ncu:u:test && npm run ncu:u:demo-example",
    "ncu:u:core-cli": "cd packages/antd-management-fast-cli && npx ncu -u",
    "ncu:u:core-common": "cd packages/antd-management-fast-common && npx ncu -u",
    "ncu:u:core-component": "cd packages/antd-management-fast-component && npx ncu -u",
    "ncu:u:core-framework": "cd packages/antd-management-fast-framework && npx ncu -u",
    "ncu:u:core-rollup": "cd packages/antd-management-fast-rollup && npx ncu -u",
    "ncu:u:demo-example": "cd packages/example-antd-management-fast-framework && npx ncu -u",
    "ncu:u:test": "cd packages/antd-management-fast-test && npx ncu -u",
    "prepare": "husky install",
    "prettier": "npx prettier -c --write \"**/*\"",
    "prepublish-npm-all": "npm run change-nrm-npm",
    "publish-npm-all": "nrm ls && npm run publish:npm-all",
    "postpublish-npm-all": "npm run change-nrm-local && nrm ls",
    "publish:c": "npm run czs && lerna updated && lerna publish --yes",
    "publish:f": "npm run build:f && npm run czs && lerna updated && lerna publish --yes",
    "prepublish:lerna": "npm run change-nrm-npm",
    "publish:lerna": "lerna updated && lerna publish --yes",
    "postpublish:lerna": "npm run change-nrm-local && npm run publish-npm-all",
    "publish:npm": "cd packages/antd-management-fast-framework && nrm use npm && nrm ls && npm publish && nrm use local && nrm ls",
    "publish:v": "lerna updated && lerna publish --yes",
    "reinstall": "npm run lerna:b",
    "show:v": "cd packages/antd-management-fast-framework && npm version",
    "sleep:change-nrm": "node ./develop/assists/sleep.js",
    "start": "cd packages/example-antd-management-fast-framework && npm start",
    "start:build": "npm run build:f && npm run start",
    "update:cz:flag": "node ./develop/assists/cz.flag.refresh.js"
  },
  "dependencies": {
    "@umijs/max": "^4.0.47",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/config-lerna-scopes": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/rollup": "^6.5.1",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cssnano": "^5.1.14",
    "dayjs": "^1.11.7",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "husky": "^8.0.3",
    "lerna": "^6.4.1",
    "lint-staged": "^13.1.0",
    "npm-check-updates": "^16.6.3",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "rollup": "^3.12.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0"
  }
}
