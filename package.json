{
  "name": "lerna-antd-management-fast-framework",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "adjust-install:framework": "cd packages/antd-management-fast-framework && npm run adjust-install",
    "adjust-install:simple": "cd packages/simple-antd-management-fast-framework && npm run adjust-install",
    "build:f": "cd packages/antd-management-fast-framework && npm run build && npm run delete:dts",
    "build:fp": "cd packages/antd-management-fast-framework && npm publish",
    "build:fvp": "npm run build:f && npm run build:vp",
    "build:p": "cd packages/antd-management-fast-framework && npm publish",
    "build:start": "npm run build:f && npm start",
    "build:vp": "cd packages/antd-management-fast-framework && npm version patch && npm publish",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "clean:all": "rimraf ./yarn-error.log && rimraf ./yarn.lock && rimraf ./package-lock.json && lerna clean -y && npm run clean:core-common && npm run clean:core-component && npm run clean:core-framework && npm run clean:demo-example && npm run clean:demo-simple && rimraf ./node_modules",
    "clean:core-common": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:core-component": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:core-framework": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:core-rollup": "cd packages/antd-management-fast-rollup && npm run clean-file",
    "clean:demo-example": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:demo-simple": "cd packages/simple-antd-management-fast-framework && npm run clean-file",
    "cli": "cd packages/antd-management-fast-cli && node ./bin/cli",
    "commitlint": "npx commitlint --edit",
    "cz": "cz",
    "init:start": "cd packages/simple-antd-management-fast-framework && npm run init:start",
    "lerna": "lerna",
    "lerna:b": "npm run clean:all && husky install && git pull && pnpm install",
    "lerna:build": "lerna run --stream --sort build",
    "lerna:create": "lerna create",
    "lerna:publish": "lerna publish --yes",
    "lint-staged": "lint-staged",
    "lint:f": "cd packages/antd-management-fast-framework && npm run lint",
    "ncu:all": "npx ncu && npm run ncu:core-cli && npm run ncu:core-common && npm run ncu:core-component && npm run ncu:core-framework && npm run ncu:core-rollup && npm run ncu:demo-example && npm run ncu:demo-simple",
    "ncu:core-cli": "cd packages/antd-management-fast-cli && ncu",
    "ncu:core-common": "cd packages/antd-management-fast-common && ncu",
    "ncu:core-component": "cd packages/antd-management-fast-component && ncu",
    "ncu:core-framework": "cd packages/antd-management-fast-framework && ncu",
    "ncu:core-rollup": "cd packages/antd-management-fast-rollup && ncu",
    "ncu:demo-example": "cd packages/example-antd-management-fast-framework && ncu",
    "ncu:demo-simple": "cd packages/simple-antd-management-fast-framework && ncu",
    "ncu:u:all": "npx ncu -u && npm run ncu:u:core-cli && npm run ncu:u:core-common && npm run ncu:u:core-component && npm run ncu:u:core-framework && npm run ncu:u:core-rollup && npm run ncu:u:demo-example && npm run ncu:u:demo-simple",
    "ncu:u:core-cli": "cd packages/antd-management-fast-cli && npx ncu -u",
    "ncu:u:core-common": "cd packages/antd-management-fast-common && npx ncu -u",
    "ncu:u:core-component": "cd packages/antd-management-fast-component && npx ncu -u",
    "ncu:u:core-framework": "cd packages/antd-management-fast-framework && npx ncu -u",
    "ncu:u:core-rollup": "cd packages/antd-management-fast-rollup && npx ncu -u",
    "ncu:u:demo-example": "cd packages/example-antd-management-fast-framework && npx ncu -u",
    "ncu:u:demo-simple": "cd packages/simple-antd-management-fast-framework && npx ncu -u",
    "prepare": "husky install",
    "publish:c": "npm run scz && lerna updated && lerna publish --yes",
    "publish:f": "npm run build:f && npm run scz && lerna updated && lerna publish --yes",
    "publish:npm": "cd packages/antd-management-fast-framework && nrm use npm && nrm ls && npm publish && nrm use local && nrm ls",
    "publish:v": "lerna updated && lerna publish --yes",
    "reinstall": "npm run lerna:b",
    "scz": "git stage -A && npm run cz && git push",
    "show:v": "cd packages/antd-management-fast-framework && npm version",
    "start": "cd packages/example-antd-management-fast-framework && npm start",
    "start:build": "npm run build:f && npm run start"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@commitlint/config-lerna-scopes": "^17.4.0",
    "@commitlint/cz-commitlint": "^17.4.1",
    "commitizen": "^4.2.6",
    "husky": "^8.0.3",
    "lerna": "^6.4.0",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0"
  }
}
