{
  "name": "lerna-antd-management-fast-framework",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "adjust-install:framework": "cd packages/antd-management-fast-framework && npm run adjust-install",
    "adjust-install:simple": "cd packages/simple-antd-management-fast-framework && npm run adjust-install",
    "build:f": "cd packages/antd-management-fast-framework && npm run build && npm run delete:dts",
    "build:fp": "cd packages/antd-management-fast-framework && npm publish",
    "build:fvp": "npm run build:f && npm run build:vp",
    "build:p": "cd packages/antd-management-fast-framework && npm publish",
    "build:start": "npm run build:f && npm start",
    "build:vp": "cd packages/antd-management-fast-framework && npm version patch && npm publish",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "clean": "rimraf ./yarn-error.log && rimraf ./yarn.lock && rimraf ./package-lock.json && lerna clean -y",
    "clean:framework": "cd packages/antd-management-fast-framework && npm run clean-file",
    "clean:simple": "cd packages/simple-antd-management-fast-framework && npm run clean-file",
    "cli": "cd packages/antd-management-fast-cli && node ./bin/cli",
    "commitlint": "npx commitlint --edit",
    "cz": "cz",
    "init:start": "cd packages/simple-antd-management-fast-framework && npm run init:start",
    "lerna": "lerna",
    "lerna:b": "npm run clean && husky install && git pull && lerna bootstrap && npm run adjust-install:framework && npm run adjust-install:simple",
    "lerna:build": "lerna run --stream --sort build",
    "lerna:create": "lerna create",
    "lerna:publish": "lerna publish --yes",
    "lint-staged": "lint-staged",
    "lint:f": "cd packages/antd-management-fast-framework && npm run lint",
    "ncu:all": "ncu && npm run ncu:framework && npm run ncu:simple",
    "ncu:framework": "cd packages/antd-management-fast-framework && ncu",
    "ncu:simple": "cd packages/simple-antd-management-fast-framework && ncu",
    "ncu:u:all": "ncu -u && npm run ncu:u:framework && npm run ncu:u:simple",
    "ncu:u:framework": "cd packages/antd-management-fast-framework && ncu -u",
    "ncu:u:simple": "cd packages/simple-antd-management-fast-framework && ncu -u",
    "prepare": "husky install",
    "publish:c": "npm run scz && lerna updated && lerna publish --yes",
    "publish:f": "npm run build:f && npm run scz && lerna updated && lerna publish --yes",
    "publish:npm": "cd packages/antd-management-fast-framework && nrm use npm && nrm ls && npm publish && nrm use local && nrm ls",
    "publish:v": "lerna updated && lerna publish --yes",
    "reinstall": "npm run clean && npm run clean:framework && npm run clean:simple && npm run lerna:b && npm run build:f && npm run init:start",
    "scz": "git stage -A && npm run cz && git push",
    "show:v": "cd packages/antd-management-fast-framework && npm version",
    "start": "cd packages/simple-antd-management-fast-framework && npm start",
    "start:build": "npm run build:f && npm run start"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@commitlint/config-lerna-scopes": "^17.0.0",
    "@commitlint/cz-commitlint": "^17.0.0",
    "commitizen": "^4.2.4",
    "husky": "^8.0.1",
    "lerna": "^5.0.0",
    "lint-staged": "^12.5.0",
    "postcss": "^8.4.14",
    "postcss-less": "^6.0.0",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0"
  }
}
