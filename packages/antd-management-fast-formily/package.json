    "@formily/shared": "^2.3.1",
    "antd-management-fast-common": "workspace:*",
    "antd-management-fast-component": "workspace:*",
    "antd-management-fast-design-react": "workspace:*",
    "classnames": "^2.5.1",
    "easy-soft-dva": "^3.10.52",
    "easy-soft-utility": "^2.8.55",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
