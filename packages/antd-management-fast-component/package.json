  "scripts": {
    "build": "npx rimraf ./es && rollup --config rollup.config.compression.js --bundleConfigAsCjs",
    "postbuild": "npm run tsc:build",
    "build:rollup": "npx rimraf ./es && rollup --config rollup.config.compression.js --bundleConfigAsCjs",
